{
  "routes": [
    {
      "method": "GET",
      "path": "/contacttags",
      "handler": "contacttag.find",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/contacttags/count",
      "handler": "contacttag.count",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "GET",
      "path": "/contacttags/:id",
      "handler": "contacttag.findOne",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "POST",
      "path": "/contacttags",
      "handler": "contacttag.create",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "PUT",
      "path": "/contacttags/:id",
      "handler": "contacttag.create",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    },
    {
      "method": "DELETE",
      "path": "/contacttags/:id",
      "handler": "contacttag.delete",
      "config": {
        "policies": ["plugins::users-permissions.isAuthenticated"]
      }
    }
  ]
}
